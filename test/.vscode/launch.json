	"version": "0.2.0",
	"configurations": [

		{
			"type": "node",
			"request": "launch",
			"name": "xtran test",
			"env": {
				"DEBUG": "xtran"
			},
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/test/index.js",
		},

		{
			"type": "node",
			"request": "launch",
			"name": "xtran init",
			"env": {
				"DEBUG": "xtran"
			},
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/lib/cli.js",
			"args": ["init", "asdf"]
		},
		
		{
			"type": "node",
			"request": "launch",
			"name": "xtran cli",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/lib/cli.js",
			"args": ["--tf", "ljl", "ffffff", "tttttt"]
		},
		
		{
			"type": "node",
			"request": "launch",
			"name": "xtran list",
			"env": {
				"DEBUG": "xtran"
			},
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/lib/cli.js",
			"args": ["list"]
		}
	]
}